<!DOCTYPE JobConfiguration PUBLIC "" "JobConfig.dtd"[

<!ENTITY QstarToQZ_M1000 SYSTEM "../../common/datasets/RunII_102X_v1/2018/QstarToQZ/QstarToQZ_M-1000.xml">
<!ENTITY QstarToQZ_M1200 SYSTEM "../../common/datasets/RunII_102X_v1/2018/QstarToQZ/QstarToQZ_M-1200.xml">
<!ENTITY QstarToQZ_M1400 SYSTEM "../../common/datasets/RunII_102X_v1/2018/QstarToQZ/QstarToQZ_M-1400.xml">
<!ENTITY QstarToQZ_M1600 SYSTEM "../../common/datasets/RunII_102X_v1/2018/QstarToQZ/QstarToQZ_M-1600.xml">
<!ENTITY QstarToQZ_M1800 SYSTEM "../../common/datasets/RunII_102X_v1/2018/QstarToQZ/QstarToQZ_M-1800.xml">
<!ENTITY QstarToQZ_M2000 SYSTEM "../../common/datasets/RunII_102X_v1/2018/QstarToQZ/QstarToQZ_M-2000.xml">
<!ENTITY QstarToQZ_M2500 SYSTEM "../../common/datasets/RunII_102X_v1/2018/QstarToQZ/QstarToQZ_M-2500.xml">
<!ENTITY QstarToQZ_M3000 SYSTEM "../../common/datasets/RunII_102X_v1/2018/QstarToQZ/QstarToQZ_M-3000.xml">
<!ENTITY QstarToQZ_M4000 SYSTEM "../../common/datasets/RunII_102X_v1/2018/QstarToQZ/QstarToQZ_M-4000.xml">
<!ENTITY QstarToQZ_M4500 SYSTEM "../../common/datasets/RunII_102X_v1/2018/QstarToQZ/QstarToQZ_M-4500.xml">
<!ENTITY QstarToQZ_M5000 SYSTEM "../../common/datasets/RunII_102X_v1/2018/QstarToQZ/QstarToQZ_M-5000.xml">
<!ENTITY QstarToQZ_M6000 SYSTEM "../../common/datasets/RunII_102X_v1/2018/QstarToQZ/QstarToQZ_M-6000.xml">

]>

<!-- OutputLevel controls which messages are printed; set to VERBOSE or DEBUG for more verbosity, to WARNING or ERROR for less -->
<JobConfiguration JobName="ExampleCycleJob" OutputLevel="INFO">
    <Library Name="libSUHH2qstar"/>
    <Package Name="SUHH2qstar.par" />

   <Cycle Name="uhh2::AnalysisModuleRunner" OutputDirectory="./" PostFix="" TargetLumi="59740.0" >
   <!-- <Cycle Name="uhh2::AnalysisModuleRunner"  RunMode="PROOF" ProofServer="lite://"  ProofWorkDir="/nfs/dust/cms/user/ottjoc/proof-wd/" ProofNodes="2" OutputDirectory="./" PostFix="" TargetLumi="1" >  -->

        <!-- Verion attribute must have year in it, same as used in ntuple config, e.g. 2018, 2016v3, etc -->
        <InputData Lumi="49974.16594" NEventsMax="-1" Type="MC" Version="qstar-Z-2TeV_2018" Cacheable="False">
           &QstarToQZ_M2000;
           <InputTree Name="AnalysisTree" />
            <!-- uncomment to write tree of selected events, i.e. of those events for which the AnalysisModule::process method  returns true
            If no OutputTree is given, no events will be written.
            You must use "AnalysisTree" as Name otherwise it will be empty. -->
            <!-- <OutputTree Name="AnalysisTree" /> -->
         </InputData>
         <InputData Lumi="49952.36239" NEventsMax="-1" Type="MC" Version="qstar-Z-5TeV_2018" Cacheable="False">
           &QstarToQZ_M5000;
           <InputTree Name="AnalysisTree" />
            <!-- uncomment to write tree of selected events, i.e. of those events for which the AnalysisModule::process method  returns true
            If no OutputTree is given, no events will be written.
            You must use "AnalysisTree" as Name otherwise it will be empty. -->
            <!-- <OutputTree Name="AnalysisTree" /> -->
        </InputData>

        <UserConfig>
            <!-- define which collections to read from the input. Only specify what you need to save I/O time -->
            <Item Name="PrimaryVertexCollection" Value="offlineSlimmedPrimaryVertices" />
            <Item Name="ElectronCollection" Value="slimmedElectronsUSER" />
            <Item Name="MuonCollection" Value="slimmedMuonsUSER" />
            <Item Name="JetCollection" Value="jetsAk8Puppi" />
            <Item Name="TopJetCollection" Value="jetsAk8PuppiSubstructure_SoftDropPuppi" />
            <Item Name="GenJetCollection" Value="slimmedGenJets" />
            <Item Name="GenTopJetCollection" Value="genjetsAk8SubstructureSoftDrop" />
            <Item Name="GenParticleCollection" Value="GenParticles" />
            <Item Name="METName" Value="slimmedMETs" />

            <!-- the class name of the AnalysisModule subclasses to run: -->
            <Item Name="AnalysisModule" Value="qstarModule" />

            <!-- tell AnalysisModuleRunner NOT to use the MC event weight from SFrame; rather let
                 MCLumiWeight (called via CommonModules) calculate the MC event weight. The MC
                 event weight assigned by MCLumiWeight is InputData.Lumi / Cycle.TargetLumi. -->
            <Item Name="use_sframe_weight" Value="false" />

            <!-- configuration for MCPileupReweight (run as part of CommonModules): -->
            <Item Name="pileup_directory" Value="common/data/2018/MyMCPileupHistogram.root" />
            <Item Name="pileup_directory_data" Value="common/data/2018/MyDataPileupHistogram2018.root" />

            <!-- Some test configuration; see qstarModule.cxx for how to access it: -->
            <Item Name="TestKey" Value="TestKeyValue" />
        </UserConfig>
    </Cycle>
</JobConfiguration>
